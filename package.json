{
  "version": "v4.2.1",
  "devDependencies": {
    "autoprefixer": "^7.2.4",
    "babel-core": "^6.21.0",
    "babel-eslint": "^8.1.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.26.0",
    "browser-sync": "^2.23.3",
    "classnames": "^2.2.5",
