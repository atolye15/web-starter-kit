    "babel-eslint": "^10.0.2",
    "browser-sync": "^2.26.7",
    "classnames": "^2.2.6",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "del": "^5.0.0",
    "eslint": "^6.0.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-bump": "^3.1.3",
    "gulp-concat": "^2.6.1",
    "gulp-directory-sync": "^1.2.3",
    "gulp-imagemin": "^6.0.0",
    "gulp-newer": "^1.4.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-svgstore": "^7.0.1",
    "gulp-tap": "^1.0.1",
    "gulp-twig": "^1.2.0",
    "gulp-uglify": "^3.0.2",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "kss": "3.0.0-beta.25",
    "lazypipe": "^1.0.2",
    "minimist": "^1.2.0",
    "node-notifier": "^5.4.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "prettier": "^1.18.2",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-prettier": "^1.1.1",
    "through2": "^3.0.1",
    "uncss": "^0.17.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "private": true,
  "scripts": {
    "node_modules-chmod": "sudo chmod -R 0777 node_modules",
    "start-server": "./node_modules/.bin/browser-sync start --server",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:js": "eslint \"src/js/**/*.js\"",
    "lint:css": "stylelint --syntax scss \"src/scss/**/*.scss\"",
    "format": "yarn format:js && yarn format:css",
    "format:js": "prettier --write \"src/**/*.js\"",
    "format:css": "stylelint --fix --syntax scss \"src/scss/**/*.scss\"",
    "gulp": "gulp",
    "generate-svg-sprite": "gulp images:sprite",
    "build-styleguide": "yarn generate-svg-sprite && gulp styleguide",
    "build": "yarn lint && gulp build",
    "build-prod": "yarn build --prod",
    "deploy": "yarn build --deploy",
    "deploy-prod": "yarn build --prod --deploy",
    "start": "yarn build && gulp styleguide && gulp serve",
    "start-prod": "yarn build-prod && gulp styleguide && gulp serve --prod",
    "start-deploy": "yarn deploy && gulp styleguide && gulp serve --deploy",
    "start-deploy-prod": "yarn build-prod && gulp styleguide && gulp serve --prod --deploy",
    "publish-github": "yarn build-prod && gh-pages -d dist",
    "gulp-tasks": "gulp --tasks"
  },
  "dependencies": {
    "buttono": "^0.5.2",
    "normalize.css": "^8.0.1"
  }
}
