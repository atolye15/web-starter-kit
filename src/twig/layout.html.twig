<!DOCTYPE HTML>
<html lang="{{ locale }}">
  <head>
    <meta charset=utf-8>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name=viewport content="width=device-width, initial-scale=1">
    {% block meta %}
      <title>Default Project</title>
    {% endblock %}
    <link rel=stylesheet href="css/main{% if production %}.min{% endif %}.css">
    {% block headerStylesheets %}{% endblock %}
    {% block headerJavascripts %}{% endblock %}
  </head>
  <body>
  {% block body %}{% endblock %}

  <!-- Javascript Files -->
  <script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
  <script>window.jQuery || document.write('<script src="vendors/jquery.min.js?v2.1.3"><\/script>')</script>
  {% if production %}
  <script src="js/app.min.js"></script>
  {% else %}
  <script src="js/libs.js"></script>
  <script src="js/main.js"></script>
  {% endif %}
  {% block footerJavascripts %}{% endblock %}
  </body>
</html>
